<?xml version="1.0" ?>
<robot name="hole"
       xmlns:xacro="http://www.ros.org/wiki/xacro">

  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="red">
    <color rgba="1 0 0 1"/>
  </material>
  <material name="green">
    <color rgba="0 1 0 1"/>
  </material>
  <material name="blue">
    <color rgba="0 0 1 1"/>
  </material>
  <material name="yellow">
    <color rgba="1 1 0 1"/>
  </material>
  
  <link name="world">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>        
  </link>

  <joint name="wall_left_joint" type="fixed">
    <parent link="world"/>
    <child link="wall_left"/>
    <!-- <axis xyz="0 0 1"/> -->
    <!-- <origin rpy="0 0 1.57" xyz="0 -0.5 0"/> -->
  </joint>
  <link name="wall_left">
    <visual>
      <origin rpy="0 0 1.57" xyz="-0.25 0 0.0"/>
      <geometry>
        <box size="0.5 0.02 0.05"/>
      </geometry>
      <material name="blue"/>      
    </visual>
    <collision>
      <origin rpy="0 0 1.57" xyz="-0.25 0 0.0"/>
      <geometry>
        <box size="0.5 0.02 0.05"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 1.57" xyz="-0.25 0 0.0"/>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>    
  </link>


  <joint name="wall_right_joint" type="fixed">
    <parent link="world"/>
    <child link="wall_right"/>
    <!-- <axis xyz="0 0 1"/> -->
    <!-- <origin rpy="0 0 1.57" xyz="0 -0.5 0"/> -->
  </joint>
  <link name="wall_right">
    <visual>
      <origin rpy="0 0 1.57" xyz="0.25 0 0.0"/>
      <geometry>
        <box size="0.5 0.02 0.05"/>
      </geometry>
      <material name="blue"/>      
    </visual>
    <collision>
      <origin rpy="0 0 1.57" xyz="0.25 0 0.0"/>
      <geometry>
        <box size="0.5 0.02 0.05"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 1.57" xyz="0.25 0 0.0"/>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>    
  </link>


  <joint name="wall_up_joint" type="fixed">
    <parent link="world"/>
    <child link="wall_up"/>
    <!-- <axis xyz="0 0 1"/> -->
    <!-- <origin rpy="0 0 1.57" xyz="0 -0.5 0"/> -->
  </joint>
  <link name="wall_up">
    <visual>
      <origin rpy="0 0 0" xyz="0 0.25 0.0"/>
      <geometry>
        <box size="0.5 0.02 0.05"/>
      </geometry>
      <material name="blue"/>      
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0.25 0.0"/>
      <geometry>
        <box size="0.5 0.02 0.05"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0.25 0.0"/>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>    
  </link>


  <joint name="hole_left_joint" type="fixed">
    <parent link="world"/>
    <child link="hole_left"/>
  </joint>
  <link name="hole_left">
    <visual>
      <origin rpy="0 0 0" xyz="-0.1525 -0.25  0.0"/>
      <geometry>
        <box size="0.195 0.05 0.05"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="-0.1525 -0.25 0.0"/>
      <geometry>
        <box size="0.195 0.05 0.05"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <joint name="hole_right_joint" type="fixed">
    <parent link="world"/>
    <child link="hole_right"/>
  </joint>
  <link name="hole_right">
    <visual>
      <origin rpy="0 0 0" xyz="0.1525 -0.25  0.0"/>
      <geometry>
        <box size="0.195 0.05 0.05"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.1525 -0.25 0.0"/>
      <geometry>
        <box size="0.195 0.05 0.05"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <joint name="hole_down_joint" type="fixed">
    <parent link="world"/>
    <child link="hole_down"/>
    <!-- <axis xyz="0 0 1"/> -->
    <!-- <origin rpy="0 0 1.57" xyz="0 -0.25 0"/> -->
  </joint>
  <link name="hole_down">
    <visual>
      <origin rpy="0 0 0" xyz="0 -0.2625 0.0"/>
      <geometry>
        <box size="0.11 0.025 0.05"/>
      </geometry>
      <material name="blue"/>            
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 -0.2625 0.0"/>
      <geometry>
        <box size="0.11 0.025 0.05"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0.0"/>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>    
  </link>


  <!-- target -->
  <joint name="target_joint" type="fixed">
    <parent link="world"/>
    <child link="target"/>
    <origin xyz="0 -0.2375 0" rpy="0.0 0.0 0.0"/>
  </joint>
  <link name="target">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.1 0.02 0.05"/>
      </geometry>
      <material name="yellow"/>            
    </visual>
    <inertial>
      <origin rpy="0 0 0"/>
      <mass value="0.00001"/>
      <inertia ixx="1." ixy="0" ixz="0" iyy="1." iyz="0.0"
               izz="1."/>
    </inertial>
  </link>



</robot>

