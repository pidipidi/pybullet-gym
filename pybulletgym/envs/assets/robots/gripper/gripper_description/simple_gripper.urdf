<?xml version="1.0" ?>
<robot name="gripper"
       xmlns:xacro="http://www.ros.org/wiki/xacro">

  <material name="red">
    <color rgba="1 0 0 1"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="red">
    <color rgba="1 0 0 1"/>
  </material>
  <material name="green">
    <color rgba="0 1 0 1"/>
  </material>
  <material name="blue">
    <color rgba="0 0 1 1"/>
  </material>
  
  <link name="world">
    <inertial>
      <origin rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="1." ixy="0" ixz="0" iyy="1." iyz="0.0"
               izz="1."/>
    </inertial>
  </link>

  <joint name="x_slider" type="prismatic">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="world" />
    <child link="x_axis_link" />
    <axis xyz="1 0 0" />
    <limit effort="1000" lower="-0.25" upper="0.25"
           velocity="0.01" />
    <!--<dynamics damping="100.0" />-->
  </joint>
  <link name="x_axis_link">
    <inertial>
      <origin rpy="0 0 0"/>
      <mass value="0.00001"/>
      <inertia ixx="28054E-9" ixy="0" ixz="0" iyy="28054E-9" iyz="0.0"
               izz="28054E-9"/>
    </inertial>
  </link>

  
  <joint name="y_slider" type="prismatic">
    <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
    <parent link="x_axis_link" />
    <child link="y_axis_link" />
    <axis xyz="0 1 0" />
    <limit effort="1000" lower="-0.25" upper="0.25"
           velocity="0.01" />
    <!--<dynamics damping="100.0" />-->
  </joint>
  <link name="y_axis_link">
    <inertial>
      <origin rpy="0 0 0"/>
      <mass value="0.00001"/>
      <inertia ixx="28054E-9" ixy="0" ixz="0" iyy="28054E-9" iyz="0.0"
               izz="28054E-9"/>
    </inertial>
  </link>
  
  <joint name="z_axis_joint" type="revolute">
    <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
    <parent link="y_axis_link" />
    <child link="palm" />
    <axis xyz="0 0 1" />
    <limit effort="1000" lower="-3.14" upper="3.14"
           velocity="0.5" />
    <!--<dynamics damping="100.0" />-->
  </joint>
  
  <link name="palm">
    <inertial>
      <origin rpy="0 0 0"/>
      <mass value="10.0001"/>
      <inertia ixx="28054E-9" ixy="0" ixz="0" iyy="28054E-9" iyz="0.0"
               izz="28054E-9"/>
    </inertial>
  </link>



  <!--left finger-->
  <joint name="left_link_joint" type="fixed">
    <origin xyz="0 0.005 0" rpy="0.0 0.0 0.0"/>
    <parent link="palm" />
    <child link="left_knuckle" />
  </joint>  
  <link name="left_knuckle">
    <visual>
      <origin rpy="0 0 0" xyz="0.01 0.01 0"/>
      <geometry>
        <box size="0.01 0.02 0.05"/>
      </geometry>
      <material name="red"/>            
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.01 0.01 0"/>
      <geometry>
        <box size="0.01 0.02 0.05"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0"/>
      <mass value="0.5"/>
      <inertia ixx="28054E-9" ixy="0" ixz="0" iyy="28054E-9" iyz="0.0"
               izz="28054E-9"/>
    </inertial>
  </link> 
  <joint name="left_finger_joint" type="prismatic">
    <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
    <parent link="left_knuckle" />
    <child link="left_finger" />
    <axis xyz="0 1 0" >
      <dynamics>
        <damping>0</damping>
        <friction>0</friction>
        <spring_reference>0</spring_reference>
        <spring_stiffness>0</spring_stiffness>
      </dynamics>
    </axis>      
    <limit effort="0" lower="0.005" upper="0.03"
           velocity="0" />
  </joint>  
  <link name="left_finger">
    <contact>
      <lateral_friction value="0.8"/>
      <rolling_friction value="0"/>
      <spinning_friction value="0.1"/>
      <!-- <inertia_scaling value="0.1"/> -->
      <!-- <stiffness value="100"/> -->
      <!-- <damping value="0.1"/> -->
    </contact>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.01 0.01 0.05"/>
      </geometry>
      <material name="red"/>            
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.01 0.01 0.05"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0"/>
      <mass value="0.2"/>
      <inertia ixx="28054E-9" ixy="0" ixz="0" iyy="28054E-9" iyz="0.0"
               izz="28054E-9"/>
    </inertial>
  </link>


  <!--right finger-->
  <joint name="right_link_joint" type="fixed">
    <origin xyz="0 -0.005 0" rpy="0.0 0.0 0.0"/>
    <parent link="palm" />
    <child link="right_knuckle" />
  </joint>  
  <link name="right_knuckle">
    <visual>
      <origin rpy="0 0 0" xyz="0.01 -0.01 0"/>
      <geometry>
        <box size="0.01 0.02 0.05"/>
      </geometry>
      <material name="red"/>            
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.01 -0.01 0"/>
      <geometry>
        <box size="0.01 0.02 0.05"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0"/>
      <mass value="0.5"/>
      <inertia ixx="28054E-9" ixy="0" ixz="0" iyy="28054E-9" iyz="0.0"
               izz="28054E-9"/>
    </inertial>
  </link> 
  <joint name="right_finger_joint" type="prismatic">
    <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
    <parent link="right_knuckle" />
    <child link="right_finger" />
    <axis xyz="0 -1 0">
      <dynamics>
        <damping>0</damping>
        <friction>0</friction>
        <spring_reference>0</spring_reference>
        <spring_stiffness>0</spring_stiffness>
      </dynamics>
    </axis>
    <limit effort="0" lower="0.005" upper="0.03"
           velocity="0" />
  </joint>  
  <link name="right_finger">
    <contact>
      <lateral_friction value="0.8"/>
      <rolling_friction value="0"/>
      <spinning_friction value="0.1"/>
    </contact>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.01 0.01 0.05"/>
      </geometry>
      <material name="red"/>            
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.01 0.01 0.05"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0"/>
      <mass value="0.2"/>
      <inertia ixx="28054E-9" ixy="0" ixz="0" iyy="28054E-9" iyz="0.0"
               izz="28054E-9"/>
    </inertial>
  </link>


  
</robot>

