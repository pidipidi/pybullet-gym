<?xml version="1.0" ?>
<robot name="hole"
       xmlns:xacro="http://www.ros.org/wiki/xacro">

  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="red">
    <color rgba="1 0 0 1"/>
  </material>
  <material name="green">
    <color rgba="0 1 0 1"/>
  </material>
  <material name="blue">
    <color rgba="0 0 1 1"/>
  </material>
  
  <link name="world">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>        
  </link>

  <joint name="wall_left_joint" type="fixed">
    <parent link="world"/>
    <child link="wall_left"/>
    <!-- <axis xyz="0 0 1"/> -->
    <!-- <origin rpy="0 0 1.57" xyz="0 -0.5 0"/> -->
  </joint>
  <link name="wall_left">
    <visual>
      <origin rpy="0 0 1.57" xyz="-0.5 0 0.0"/>
      <geometry>
        <box size="1.0 0.053 0.053"/>
      </geometry>
      <material name="blue"/>      
    </visual>
    <collision>
      <origin rpy="0 0 1.57" xyz="-0.5 0 0.0"/>
      <geometry>
        <box size="1.0 0.053 0.053"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 1.57" xyz="-0.5 0 0.0"/>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>    
  </link>


  <joint name="wall_right_joint" type="fixed">
    <parent link="world"/>
    <child link="wall_right"/>
    <!-- <axis xyz="0 0 1"/> -->
    <!-- <origin rpy="0 0 1.57" xyz="0 -0.5 0"/> -->
  </joint>
  <link name="wall_right">
    <visual>
      <origin rpy="0 0 1.57" xyz="0.5 0 0.0"/>
      <geometry>
        <box size="1.0 0.053 0.053"/>
      </geometry>
      <material name="blue"/>      
    </visual>
    <collision>
      <origin rpy="0 0 1.57" xyz="0.5 0 0.0"/>
      <geometry>
        <box size="1.0 0.053 0.053"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 1.57" xyz="0.5 0 0.0"/>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>    
  </link>


  <joint name="wall_up_joint" type="fixed">
    <parent link="world"/>
    <child link="wall_up"/>
    <!-- <axis xyz="0 0 1"/> -->
    <!-- <origin rpy="0 0 1.57" xyz="0 -0.5 0"/> -->
  </joint>
  <link name="wall_up">
    <visual>
      <origin rpy="0 0 0" xyz="0 0.5 0.0"/>
      <geometry>
        <box size="1.0 0.053 0.053"/>
      </geometry>
      <material name="blue"/>      
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0.5 0.0"/>
      <geometry>
        <box size="1.0 0.053 0.053"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0.5 0.0"/>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>    
  </link>


  <joint name="wall_down_joint" type="fixed">
    <parent link="world"/>
    <child link="wall_down"/>
    <!-- <axis xyz="0 0 1"/> -->
    <!-- <origin rpy="0 0 1.57" xyz="0 -0.5 0"/> -->
  </joint>
  <link name="wall_down">
    <visual>
      <origin rpy="0 0 0" xyz="0 -0.53 0.0"/>
      <geometry>
        <box size="1.0 0.053 0.053"/>
      </geometry>
      <material name="blue"/>            
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 -0.53 0.0"/>
      <geometry>
        <box size="1.0 0.053 0.053"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 -0.53 0.0"/>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>    
  </link>


  <joint name="hole_left_joint" type="fixed">
    <parent link="world"/>
    <child link="hole_left"/>
  </joint>
  <link name="hole_left">
    <visual>
      <origin rpy="0 0 0" xyz="-0.28 -0.45  0.0"/>
      <geometry>
        <box size="0.44 0.1 0.053"/>
      </geometry>
      <material name="red"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="-0.28 -0.45 0.0"/>
      <geometry>
        <box size="0.44 0.1 0.053"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <joint name="hole_right_joint" type="fixed">
    <parent link="world"/>
    <child link="hole_right"/>
  </joint>
  <link name="hole_right">
    <visual>
      <origin rpy="0 0 0" xyz="0.28 -0.45  0.0"/>
      <geometry>
        <box size="0.44 0.1 0.053"/>
      </geometry>
      <material name="red"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.28 -0.45 0.0"/>
      <geometry>
        <box size="0.44 0.1 0.053"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>



</robot>

